@model IEnumerable<MvcPL.Models.TestViewModel>
@if (Model.Count() == 0)
{
    <h3 class="text-error">No items matched your search query!</h3>
}
else
{
    foreach (var result in Model)
    {
        <div class="container">
            <ul>
                <li class="list-group-item">
                    @Html.ActionLink(result.Name, "Test", new { id = result.Id })
                </li>
            </ul>
        </div>
    }
}
